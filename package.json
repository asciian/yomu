{
  "name": "yomu",
  "version": "1.3.1",
  "description": "PDF viewer (with English Japanese dictionary)",
  "main": "src/main.js",
  "author": "TANIGUCHI Masaya",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rm -rf dist **/*~",
    "dist-clean": "npm run clean && rm -rf node_modules",
    "dist": "electron-builder -mwl",
    "pack": "electron-builder -mwl --dir"
  },
  "devDependencies": {
    "about-window": "^1.13.4",
    "electron": "^11.0.3",
    "electron-builder": "^22.9.1"
  },
  "build": {
    "appId": "casa.docs.yomu",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "linux": {
      "category": "Office"
    },
    "publish": {
      "provider": "github",
      "owner": "tani",
      "repo": "yomu",
      "releaseType": "release"
    }
  },
  "bin": "bin/cli.js",
  "dependencies": {}
}
